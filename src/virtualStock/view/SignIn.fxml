<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="virtualStock.controller.SignInController">
   <children>
      <ButtonBar layoutX="54.0" layoutY="260.0" prefHeight="40.0" prefWidth="159.0" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="66.0">
