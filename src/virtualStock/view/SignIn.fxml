        </buttons>
      </ButtonBar>
      <TextField fx:id="usernameTextField" layoutX="76.0" layoutY="150.0" onAction="#handleUsername" promptText="Username" text="jaykim" />
      <PasswordField fx:id="passwordPasswordField" layoutX="76.0" layoutY="212.0" onAction="#handlePassword" promptText="Password" text="password" />
      <Text layoutX="75.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Virtual Stock">
         <font>
            <Font size="27.0" />
         </font>
      </Text>
      <Text layoutX="177.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Trading">
         <font>
            <Font size="23.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
