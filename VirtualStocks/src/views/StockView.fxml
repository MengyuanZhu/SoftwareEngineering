<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.StockController">
   <children>
      <Text fx:id="stockInfoText" layoutX="14.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Stock Information" />
      <Button fx:id="buyButton" layoutX="129.0" layoutY="188.0" mnemonicParsing="false" onAction="#handleBuy" text="Buy" />
      <HBox alignment="CENTER" layoutX="-2.0" layoutY="8.0" prefHeight="47.0" prefWidth="300.0">
         <children>
            <Text fx:id="stockNameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Stock Name">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
         </children>
      </HBox>
   </children>
</AnchorPane>
